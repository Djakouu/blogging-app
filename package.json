  },
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "env-cmd": "8.0.2",
    "graphql-cli": "^3.0.11",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "^8.5.1",
    "prisma-binding": "2.1.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.0"
  }
}
